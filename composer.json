{
  "name": "luqmannasir55/yasumi",
  "type": "project",
  "description": "...",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/luqmannasir55/yasumi"
    }
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sacha Telgenhof",
      "email": "me@sachatelgenhof.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.*",
    "azuyalabs/yasumi": "dev-master"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "fzaninotto/faker": "~1.8",
    "mikey179/vfsstream": "~1.6",
    "phan/phan": "^2.2",
    "phpunit/phpunit": "~7.5"
  },
  "autoload": {
    "psr-4": {
      "Yasumi\\": "src/Yasumi/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yasumi\\tests\\": "tests/"
    }
  },
  "scripts": {
    "format": "./vendor/bin/php-cs-fixer fix .",
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
